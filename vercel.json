{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "env": {
        "NODE_VERSION": "16"
      }
    }
  ],
  "routes": [
    {
      "src": "/socket.io/(.*)", 
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}